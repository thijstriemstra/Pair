<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://ns.adobe.com/air/application/@air.runtime.version@" 
			 appId="@air.id@" 
			 version="@air.app.version@">

      <name>@air.name@</name>
      
      <description>@air.description@</description>
      
      <copyright>@air.copyright@</copyright>
      
      <initialWindow>
	
	<title>@air.title@</title>
	
	<content>@air.swf@</content>

	<systemChrome>@air.systemChrome@</systemChrome>

	<transparent>@air.transparent@</transparent>

	<visible>@air.visible@</visible>

	<minimizable>@air.minimizable@</minimizable>
  	<maximizable>@air.maximizable@</maximizable>
  	<resizable>@air.resizable@</resizable>

  	<width>@air.width@</width>
  	<height>@air.height@</height>
  	<x>@air.x@</x>
  	<y>@air.y@</y>
  	<minSize>@air.minSize@</minSize>
  	<maxSize>@air.maxSize@</maxSize>

      </initialWindow>

      <installFolder>@air.installFolder@</installFolder>
	  
      <icon>
          <image16x16>@air.image16x16@</image16x16>
          <image32x32>@air.image32x32@</image32x32>
          <image48x48>@air.image48x48@</image48x48>
          <image128x128>@air.image128x128@</image128x128> 
      </icon>
      
</application>